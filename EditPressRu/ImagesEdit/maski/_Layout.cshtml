    <script type="text/javascript">

        function ShowModalWin(e, text) {
            $("#modal_message").text(text);
            var id = $(this).attr('href');
            var maskHeight = $(document).height();
            var maskWidth = $(window).width();
            $('#mask').css({ 'width': maskWidth, 'height': maskHeight });
            $('#mask').fadeIn(1000);
            $('#mask').fadeTo("slow", 0.8);
            var winH = $(window).height();
            var winW = $(window).width();
            $("#dialog").css('top', winH / 2 - $("#dialog").height() / 2);
            $("#dialog").css('left', winW / 2 - $("#dialog").width() / 2);
            $("#dialog").fadeIn(2000);
        };

        $(document).ready(function () {
            $('.window .close').click(function (e) {
                e.preventDefault();
                $('#mask, .window').hide();
            });
            $('#mask').click(function () {
                $(this).hide();
                $('.window').hide();
            });
        });

        $(document).ready(function () {
            $("#call-order-label").click(function () {
                event.preventDefault();
                $("#call-order").toggle("slow");
            });
            $("#call-order-form").submit(function () {
                event.preventDefault();
                var formData = new FormData($(this)[0]);
                $.ajax({
                    url: "/Home/SendCallOrder",
                    type: "POST",
                    data: formData,
                    async: false,
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function (returndata) {
                        $("#call-order").toggle("slow");
                    }
                });
            });

            $("#get-opt-price").submit(function () {
                event.preventDefault();
                var formData = new FormData($(this)[0]);
                $.ajax({
                    url: "/Home/GetOptPrice",
                    type: "POST",
                    data: formData,
                    async: false,
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function (returndata) {
                        ShowModalWin(Event, "Запрос отправлен");
                    }
                });
            });

            $("#subscription-submit").click(function () {

                var formData = $("#subscription-email").val();
                $.ajax({
                    url: "/Home/SendSuscription",
                    type: "GET",
                    data: "email=" + formData,
                    async: false,
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function (returndata) {
                        ShowModalWin(Event, "Запрос отправлен");
                    }
                });
            });

        });
       
    </script>
