        function GetCartSumm() {
            $.ajax({
                url: "/Cart/GetCartSumm",
                type: "POST",
                processData: false,
                mimeType: "text/html",
                data: "",
                dataType: "json",
                success: function (returndata) {
                    if (returndata.Result == true) {
                        $(".cart-empty a img").hide();
                        $(".cart-empty a.CartIndex").text(new String(returndata.summa) + " руб.");
                    }
                    else {
                        $(".cart-empty a img").show();
                        $(".cart-empty a.CartIndex").text("Корзина пуста");
                    }
                }
            });
        }

        $(document).ready(GetCartSumm);


