                $.ajax({
                    type: 'GET',
                    cache: false,
                    url: '/cart/getcartsumm',
                    dataType: 'json',
                    data: {},
                    error: function () {
                    },
                    success: function (returndata) {

                        if (returndata.Result == true) {

                            //$("#cnt_tovar").text(new String(returndata.total));
                            $("#sum_tovar").text(new String(returndata.summa) + " Р");
                            if (returndata.isa === 1) {
                                $('#link_isa').attr('href', '/orders/editorder/' + returndata.orderId);
                            }
                            else {
                                $('#link_una').attr('href', '/orders/editorder/' + returndata.orderId);
                            }

                        }
                        else {
                            //$("#cnt_tovar").text("0");
                            $("#sum_tovar").text("0 Р");

                        }
                        //alert("from getCartSumm");
                    }
                });
            }

            $('.btn-menu').click(function (eventObject) {
                $(this).toggleClass('active');
